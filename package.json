{
	"name": "Senutila",
	"version": "0.1.0",
	"description": "A Discord bot framework focused on providing a great developer experience. Also better than discord.js.",
	"main": "lib/index.js",
	"files": [
		"lib",
		"package.json"
	],
	"scripts": {
		"pre-build": "rimraf lib/*",
		"build": "pnpm run pre-build&&tsc",
		"pre-build-docs": "rimraf docs/api/*",
		"build-docs": "pnpm run pre-build-docs&&typedoc",
		"dev": "tsc --watch --incremental",
		"pre-build-example": "rimraf example/*.js",
		"build-example": "pnpm run pre-build-example&&tsc --project tsconfig.example.json"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/edazpotato/senutila.git"
	},
	"keywords": [
		"discord",
		"bot",
		"robotics",
		"cli",
		"backend"
	],
	"author": "https://github.com/edazpotato",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/edazpotato/senutila/issues"
	},
	"homepage": "https://github.com/edazpotato/senutila#readme",
	"dependencies": {
		"@discordjs/collection": "^0.2.1",
		"@discordjs/rest": "^0.1.0-canary.0",
		"bufferutil": "^4.0.3",
		"centra": "^2.5.0",
		"chalk": "^4.1.1",
		"discord-api-types": "^0.22.0",
		"dotenv": "^10.0.0",
		"erlpack": "^0.1.3",
		"tweetnacl": "^1.0.3",
		"utf-8-validate": "^5.0.5",
		"ws": "^8.0.0",
		"zlib-sync": "^0.1.7"
	},
	"devDependencies": {
		"@types/centra": "^2.2.0",
		"@types/node": "^16.4.12",
		"@types/ws": "^7.4.7",
		"rimraf": "^3.0.2",
		"typedoc": "^0.21.5",
		"typedoc-plugin-markdown": "^3.10.4",
		"typescript": "^4.3.5"
	},
	"engines": {
		"node": ">=16"
	}
}
