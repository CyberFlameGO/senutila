{
	"name": "senutila",
	"version": "0.0.8",
	"description": "A Discord bot framework that's focused on making the developer experience great. Also better than Discord.JS",
	"main": "lib/index.js",
	"files": [
		"lib",
		"package.json"
	],
	"scripts": {
		"pre-build": "rimraf lib/*",
		"build": "pnpm run pre-build&&tsc",
		"dev": "tsc --watch --incremental",
		"pre-build-example": "rimraf example/*.js",
		"build-example": "pnpm run pre-build-example&&tsc --project tsconfig.example.json"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/edazpotato/senutila.git"
	},
	"keywords": [
		"discord",
		"bot",
		"robotics",
		"cli",
		"backend"
	],
	"author": "https://github.com/edazpotato",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/edazpotato/senutila/issues"
	},
	"homepage": "https://github.com/edazpotato/senutila#readme",
	"dependencies": {
		"bufferutil": "^4.0.3",
		"centra": "^2.5.0",
		"chalk": "^4.1.1",
		"discord-api-types": "^0.20.2",
		"erlpack": "github:discord/erlpack",
		"tweetnacl": "^1.0.3",
		"utf-8-validate": "^5.0.5",
		"ws": "^7.5.2",
		"zlib-sync": "^0.1.7"
	},
	"devDependencies": {
		"@discordjs/rest": "^0.1.0-canary.0",
		"@types/centra": "^2.2.0",
		"@types/node": "^16.4.12",
		"@types/ws": "^7.4.7",
		"rimraf": "^3.0.2",
		"typescript": "^4.3.5"
	},
	"engines": {
		"node": ">=16"
	}
}
